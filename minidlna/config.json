{
  "name": "MiniDLNA",
  "version": "0.2.6",
  "slug": "minidlna",
  "description": "Install ReadyMedia (MiniDLNA) to your Hass.io host",
  "url": "https://github.com/zoroz/addon-torrdlna",
  "startup": "services",
  "boot": "auto",
  "hassio_api": true,
  "homeassistant_api": false,
  "host_network": true,
  "ports": {
    "8200/tcp": 8200,
    "1900/udp": 1900
  },
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": [
    "config:rw",
    "media:rw",
    "share:rw",
    "ssl"
  ],
  "options": {
  },
  "schema": {
  },
  "devices": ["/dev/net/tun"],
  "privileged": ["NET_ADMIN"]
}