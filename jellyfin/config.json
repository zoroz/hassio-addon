{
  "name": "jellyfin",
  "version": "1.3.0",
  "slug": "jellyfin",
  "legacy": false,
  "description": "A Free Software Media System that puts you in control of managing and streaming your media.",
  "url": "https://github.com/petersendev/hassio-addons/tree/master/jellyfin#readme",
  "startup": "services",
  "ingress": "true",
  "ingress_port": 8096,
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "ports": {
    "8096/tcp": 8096
  },
  "ports_description": {
    "8096/tcp": "web interface"
  },
  "map": [
    "config:rw",
    "share:rw",
    "media:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:8096]",
  "boot": "auto",
  "options": {
    "PUID": 1001,
    "PGID": 1001
  },
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?"
  }
}
